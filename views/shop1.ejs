<script>
  function doMathSC(i) {

    var quantities = document.getElementsByClassName('scqty');
    var scLength = document.getElementById('scLength').value;

    // To calculate row wise subtotal and actual price
    var numOne = document.getElementById('1priceValue_'+i).value;
    var numTwo = quantities[i].value;
    var numThree = document.getElementById('1actualValue_'+i).value;
    var discValue = Math.round(Number(numOne) * Number(numTwo));
    var actValue = Number(numThree) * Number(numTwo);
    console.log("Act value :" + actValue);
    document.getElementById('1tempTotal_'+i).value = discValue;
    document.getElementById('1actSubTotal_'+i).value = actValue;

    // To calculate the total amount of Sound Crackers

    var netSCValues = 0;
    for(i=0; i<scLength;i++){
      Math.round(netSCValues += Number(document.getElementById('1tempTotal_'+i).value));
    }

    document.getElementById('netSCValues').value = netSCValues;

    // To calculate total products quantity
    var qty = document.getElementsByClassName("scqty");
    var tempQty = 0;
    for(j=0; j<scLength;j++){
      tempQty += Number(scqty[j].value);
    }
    document.getElementById('scTotalProducts').value =  tempQty;

    // To Calculate the discount amount
    var originalPrice=0;
    var discPrice=0;

    for(i=0;i<scLength;i++){
      originalPrice += Number(document.getElementById('1actSubTotal_'+i).value);
      discPrice += Number(document.getElementById('1tempTotal_'+i).value);
    }
    var discountedValue = originalPrice - discPrice;
    document.getElementById('scdiscountTotal').value = discountedValue.toFixed(2);
    document.getElementById('scActualPrice').value = originalPrice.toFixed(2);
    
    calcOverallTotal();

  }

